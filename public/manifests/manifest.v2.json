{
  "manifest_version": 2,
  "name": "DubsDubs",
  "version": "1.0.0",
  "description": "Adds custom subtitles to YouTube's default subtitle interface",
  "permissions": ["*://*.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": ["index.html"]
}
